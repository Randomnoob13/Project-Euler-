package Problem1;

public class VielfachesVonDreiUndFuenf {

	public static void main(String[] args) {
// Wenn wir alle nat√ºrlichen Zahlen unter 10 auflisten, die ein Vielfaches von 3 oder 5 sind, so erhalten wir 3, 5, 6 und 9. Die Summe dieser Vielfachen ist 23.
//Finden Sie die Summe aller Vielfachen von 3 oder 5 unter 1000.
		int n = 1000;
		int sum = 0;

		for (int i = 0; i < n; i++) {
			if ((i % 3 == 0) || (i % 5 == 0)) {
				sum += i;
			}
		}
		System.out.println(sum);
	}

}
